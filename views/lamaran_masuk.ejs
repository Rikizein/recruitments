                <div class="row">
                    <!-- metric -->

                <!-- ============================================================== -->
                    <!-- data table  -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="mb-0">Biodata Pelamar</h5>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="example" class="table table-striped table-bordered second" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th class="text-center">No.</th>
                                                <th class="text-center">Name</th>
                                                <th class="text-center">Tempat/tanggal lahir</th>
                                                <th class="text-center">Posisi Yang Di Lamar</th>
                                                <th class="text-center">Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for(var i = 0; i < data.length; i++){ %>
                                            <tr>
                                                <td class="text-center"><%= i + 1 %></td>
                                                <td><%= data[i].nama %></td>
                                                <td><%= data[i].tempatLahir %> / <%= data[i].tglLahir.toDateString() %></td>
                                                <td><%= data[i].posisi %></td>
                                                <td class="text-center">
                                                    <span><a href="/profile/<%=data[i].id%>"
                                                            title="Lihat"><i class="fa fa-search"></i></a></span>
                                                    <!-- <span><a href="/delete_data/<%=data[i].id%>" title="delete"
                                                            onclick="return confirm(`Yakin akan hapus <%=data[i].nama%>?`);"><i
                                                                class="fas fa-trash-alt"></i> </a></span> -->
                                                </td>
                                            </tr>
                                            <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
